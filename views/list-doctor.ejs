         <!-- Left Sidebar End --><!-- ============================================================== --><!-- Start right Content here --><!-- ============================================================== -->
         <div class="content-page">
            <!-- Start content -->
            <div class="content">
               <div class="container-fluid">
                  <!-- end row -->
                  <div class="row">
                     <!-- end col -->
                     <div class="col-lg-12">
                        <div class="card m-b-20">
                           <div class="card-body">
                              <div class="row">
                                  <div class="col-md-10">
                                        <h4 class="mt-0 header-title">Danh Sách Bác Sĩ</h4>
                                  </div>
                                  <div class="col-md-2">
                                      <a href="/tao-ho-so">
                                        <button type="button" class="btn btn-outline-dark waves-effect waves-light">Cập Nhật Mới</button>
                                      </a>
                                  </div>
                              </div>
                              <p class="text-muted m-b-30">Danh Sách Bác Sĩ.</p>
                              <table class="table table-dark mb-0">
                                 <thead>
                                    <tr>
                                       <th>#</th>
                                       <th>Họ Tên</th>
                                       <th>Ngày Sinh</th>
                                       <th>Giới Tính</th>
                                       <th>Điện Thoại</th>
                                    </tr>
                                 </thead>
                                 <tbody>
                                    <% var flag = 1; %>
                                    <%listDoctor.forEach(item => {%>
                                        <tr>
                                            <th scope="row"><%= flag %></th>
                                            <td><%= item.fullname %></td>

                                            <% var convertTime = moment(item.birthday).subtract(10, 'days').calendar(); %>
                                            <td><%= convertTime%></td>

                                            <td>
                                                <%if(Number(item.gender) === 1) {%>
                                                    <span class="badge badge-pill badge-info">Nam</span>
                                                <%}else{%>
                                                    <span class="badge badge-pill badge-danger">Nữ</span>
                                                <%}%>
                                            </td>
                                           
                                            <td><%= item.phone%></td>
                                        </tr>
                                        <% flag++;%>
                                    <%})%>
                                   
                                 </tbody>
                              </table>
                           </div>
                        </div>
                     </div>
                     <!-- end col -->
                  </div>
               </div>
